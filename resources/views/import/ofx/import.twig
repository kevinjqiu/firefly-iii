{% extends "./layout/default" %}

{% block breadcrumbs %}
    {{ Breadcrumbs.renderIfExists(Route.getCurrentRoute.getName, job) }}
{% endblock %}

{% block content %}

    <form class="form-horizontal" action="{{ route('import.file.start', job.key) }}" method="post" enctype="multipart/form-data">
        <input type="hidden" name="_token" value="{{ csrf_token() }}"/>
        <div class="row">
            <div class="col-lg-12">
                <div class="box">
                    <div class="box-body">
                        <button type="submit" class="pull-right btn btn-success">
                            {{ trans('ofx.submit') }}
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </form>




{% endblock %}
